{
  "manifest_version": 2,
  "name": "GitHub QuickNav for Chrome",
  "description": "Quickly navigate between your GitHub repositories. Created for hack week! Not an official GitHub project.",
  "author": "Damien Guard",
  "version": "1.0",
  "icons": {
    "32": "images/github-32.png",
    "64": "images/github-64.png",
    "128": "images/github-128.png"
  },
  "browser_action": {
    "default_icon": "images/github-64.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "history",
    "webNavigation",
    "tabs"
  ]
}
